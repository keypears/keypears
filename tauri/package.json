    "prettier-plugin-tailwindcss": "^0.6.14",
    "react-router-devtools": "^5.1.3",
    "sharp": "^0.34.4",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vite-tsconfig-paths": "^5.1.4",
    "wait-on": "9.0.1"
